{
    "tasks": {
     "play": "deno run -A play.ts",
     "print-feeds": "deno run -A print-feeds.ts",
     "server": "deno run -A server.ts"
    },
     "fmt": {
         "useTabs": false,
         "lineWidth": 120,
         "indentWidth": 2,
         "semiColons": true,
         "singleQuote": true,
         "proseWrap": "preserve",
         "include": ["*.ts","lib/**/*.ts","lib/services/*.ts"],
         "exclude": ["src/testdata/", "data/fixtures/**/*.ts"]
       }
     }